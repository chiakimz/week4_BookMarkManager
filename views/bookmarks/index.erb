<%= styled_flash %>

<ul>
  <% @bookmarks.each do |bookmark| %>
    <li id="bookmark-<%="bookmark.id %>">
      <a href="<%= bookmark.url %>" target="_blank">
        <%=bookmark.title %>
      </a>
        <form action="/bookmarks/<%=bookmark.id %>/delete" method="post">
          <input type='submit' value='Delete' />
        </form>
    </li>
  <% end %>
</ul>
